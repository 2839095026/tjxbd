@model List<Entity.engage_resume>
@{
    Layout = null;
}

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-trasitional.dtd">
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>My JSP 'resume-list.jsp' starting page</title>
    <link rel="stylesheet"
          href="../../../css/table.css" type="text/css">
    <link rel="stylesheet"
          href="../../../css/cwcalendar.css" type="text/css">
    <script type="text/javascript"
            src="../../../javascript/comm/comm.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/comm/list.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/calendar-ch.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/jquery-1.7.2.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/locate.js">
    </script>
</head>
<body>
    <form method="post" name="fm" onsubmit="return false" action="/HR_Fist/recruit/recruitAction!toResumeList?a=list">
        <input type="hidden" name="utilBean.currPage" id="page" />
        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--招聘管理--简历管理--简历筛选--简历筛选列表
                    </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="button" value="返回" class="BUTTON_STYLE1" onclick="history.back();">
                </td>
            </tr>
        </table>
        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr>
                <td width="15%" class="TD_STYLE1">
                    档案编号
                </td>
                <td width="10%" class="TD_STYLE1">
                    姓名
                </td>
                <td width="10%" class="TD_STYLE1">
                    性别
                </td>
                <td width="15%" class="TD_STYLE1">
                    职位分类
                </td>
                <td width="5%" class="TD_STYLE1">
                    职位名称
                </td>
                <td width="5%" class="TD_STYLE1">
                    电话号码
                </td>
                <td width="5%" class="TD_STYLE1">
                    复核状态
                </td>
                <td width="5%" class="TD_STYLE1">
                    复核
                </td>
            </tr>




            @{
                foreach (var item in Model)
                {
                    <tr>
                        <td class="TD_STYLE2">
                            @item.res_id
                        </td>
                        <td class="TD_STYLE2">
                            @item.human_name
                        </td>
                        <td class="TD_STYLE2">
                            @item.human_sex
                        </td>
                        <td class="TD_STYLE2">
                            @item.human_major_kind_name
                        </td>
                        <td class="TD_STYLE2">
                            @item.human_major_name
                        </td>
                        <td class="TD_STYLE2">
                            @item.human_telephone
                        </td>
                        <td class="TD_STYLE2">

                            @UI.Controllers.ResumeController.GetCheckStatusCN((int)item.check_status)
                        </td>
                        <td class="TD_STYLE2">
                            <a href="/Resume/resumedetails/@item.res_id ">复核</a>
                        </td>
                    </tr>
                }
            }

        </table>
        <p>
            &nbsp;&nbsp;总数：@(ViewBag.count) 例 &nbsp;&nbsp;&nbsp;当前第@(ViewBag.index) 页 /每页显示 @(ViewBag.pages) 条 &nbsp;&nbsp;&nbsp;共 @( ViewBag.pages) 页 &nbsp;&nbsp;&nbsp;
            <a href="?pageindex=@(ViewBag.index>1?ViewBag.index-1:1)&keyWrods=@(Context.Request.QueryString["keyWrods"])&startTime=@(Context.Request.QueryString["startTime"])&endTime=@(Context.Request.QueryString["endTime"])&mkid=@(Context.Request.QueryString["mkid"].Replace(" ","+"))&mid=@(Context.Request.QueryString["mid"].Replace(" ","+"))&status=@(Context.Request.QueryString["status"])">上一页</a> &nbsp;&nbsp;&nbsp;
            <a href="?pageindex=@(ViewBag.index<ViewBag.pages?ViewBag.index+1:ViewBag.pages)&keyWrods=@(Context.Request.QueryString["keyWrods"])&startTime=@(Context.Request.QueryString["startTime"])&endTime=@(Context.Request.QueryString["endTime"])&mkid=@(Context.Request.QueryString["mkid"].Replace(" ","+"))&mid=@(Context.Request.QueryString["mid"].Replace(" ","+"))&status=@(Context.Request.QueryString["status"])">下一页</a> &nbsp;&nbsp;&nbsp;跳到第 <input name=page type=text class=input1 id="currPage" size=1> 页&nbsp;&nbsp;<input type=image src="../../../images/go.bmp" onclick="tj()" width=18 height=18 border=0>
    </form>

    <script>
        function tj() {
            var count=@(ViewBag.pages);
            if($('#currPage').val()>count){
                alert('目标页不存在')
                return 
            }
            var url = '?pageindex=' + $('#currPage').val() + '&keyWrods=@(Context.Request.QueryString["keyWrods"])&startTime=@(Context.Request.QueryString["startTime"])&endTime=@(Context.Request.QueryString["endTime"])&mkid=@(Context.Request.QueryString["mkid"].Replace(" ","+"))&mid=@(Context.Request.QueryString["mid"].Replace(" ","+"))';
            location.href=url
        }
    </script>
  
</body>
</html>
