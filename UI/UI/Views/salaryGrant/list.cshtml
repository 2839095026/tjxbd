@model List<Entity.salary_standard_details>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="~/css/table.css" type="text/css" />
    <script src="~/javascript/jquery-1.7.2.js"></script>
    <style type="text/css">
        <!--
        .style3 {
            color: #0000CC;
        }

        td {
            text-align: center;
        }
        -->
    </style>
    <title>list</title>
</head>
<body>
    <table width="100%">
        <tr>
            <td colspan="2" style="text-align: left">
                <font color="black">
                    您正在做的业务是：人力资源--薪酬标准管理--薪酬发放登记--工资详情
                </font>
            </td>
        <tr>
            <td colspan="2" style="text-align: right;">
                <input type="button" value="返回" onclick="javascript:window.history.back();" class="BUTTON_STYLE1">
            </td>
        </tr>
    </table>
    <table width="100%" border="1" cellpadding=0 cellspacing=1
           bordercolorlight=#848284 bordercolordark=#eeeeee
           class="TABLE_STYLE1">
        <tr>
            @foreach (Entity.salary_standard_details item in Model)
            {
                <td class="TD_STYLE1">
                    @item.item_name
                </td>
            }
        </tr>
        <tr>
            @foreach (Entity.salary_standard_details item in Model)
            {
                <td class="TD_STYLE2">
                    @item.salary
                </td>
            }
        </tr>
    </table>
</body>
</html>
