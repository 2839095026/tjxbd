@model List<Entity.config_public_char>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>public_char</title>
    <link rel="stylesheet"
          href="../../css/table.css" type="text/css">
    <script type="text/javascript"
            src="../../javascript/comm/comm.js">
    </script>

    <script type="text/javascript" src="../../javascript/jquery-1.7.2.js"></script>


</head>
<body>
    <form method="post" action="../customiz/customizAction!findPublicChar">
        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--公共属性设置
                    </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="button" value="添加" class="BUTTON_STYLE1"
                           onclick="window.location.href = '/client/public_char_add';">
                </td>
            </tr>
        </table>
        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr>
                <td width="49%" class="TD_STYLE1">
                    属性种类
                </td>
                <td width="44%" class="TD_STYLE1">
                    属性名称
                </td>
                <td width="7%" class="TD_STYLE1" align="center">
                    删除
                </td>
            </tr>
            @foreach (Entity.config_public_char dr in Model)
            {
                <tr>

                    <td class="TD_STYLE2">@dr.attribute_kind</td>
                    <td class="TD_STYLE2">@dr.attribute_name</td>   
                    <td class="TD_STYLE2">
                        <a href="/client/Deletepublic_char/@dr.pbc_id">删除</a>

                    </td>
                </tr>
            }
            </table>
        </form>
    </body>
</html>
